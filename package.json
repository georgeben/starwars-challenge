{
  "name": "starwars",
  "version": "1.0.0",
  "description": "Starwars API",
  "main": "index.js",
  "scripts": {
    "build": "babel src --delete-dir-on-start --copy-files -d dist",
    "prestart": "npm run build && npm run migrate:up",
    "start:dev": "nodemon \"src/index.js\"",
    "start": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js --env production",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html --reporter=lcov mocha --require @babel/register \"src/test/**/**/*.spec.js\" --exit --timeout 20000 --recursive",
    "lint": "node_modules/.bin/eslint \"./src/**/*.js\"",
    "docs:api": "echo 'Building REST Docs...' && apidoc -i src/interfaces/http/routes/ -o docs/apidocs",
    "migrate:up": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop"
  },
  "keywords": [],
  "author": "George Kurobara",
  "dependencies": {
    "apidoc": "^0.29.0",
    "awilix": "^4.3.3",
    "awilix-express": "^4.0.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "convict": "^6.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv-extended": "^2.9.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "helmet": "^4.5.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.1.0",
    "request-ip": "^2.1.3",
    "sequelize": "^6.5.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0"
  },
  "license": "ISC"
}
